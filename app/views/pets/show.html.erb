<p>Name: <%= @pet.name %></p>
<p>Breed: <%= @pet.breed %></p>
<p>Specie: <%= @pet.specie %></p>
<p>Birthdate: <%= @pet.birthdate %></p>
<p>Age: <%= @pet.age %></p>
<p>Gender: <%= @pet.gender %></p>
<p>Vaccinated: <%= @pet.vaccinated %></p>
<br>
<% if @pet.images? %>
  <ul>
  <% @pet.images.each do |img| %>
    <li><%= cl_image_tag("#{img.public_id}", width: 90, height: 130) %></li>
  <% end %>
  </ul>
<% end %>
<%= link_to 'back', pets_path %>



